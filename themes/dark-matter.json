{
  "$schema":"vscode://schemas/color-theme",
  "name":"Dark Matter",
  "type": "dark",
  "tokenColors":[
    {
      "name":"Comment",
      "scope":[
        "comment"
      ],
      "settings":{
        "foreground":"#ffffff80",
        "fontStyle": "italic"
      }
    },
    {
      "name":"String",
      "scope":"string",
      "settings":{
        "foreground":"#ffffff",
        "fontStyle": "bold italic"
      }
    },
    {
      "name":"Number",
      "scope":"constant.numeric",
      "settings":{
        "foreground":"#3cb9fc",
        "fontStyle": "bold italic"
      }
    },
    {
      "name":"Built-in constant",
      "scope":[
        "constant.language",
        "entity.other.attribute-name.pseudo-class"
      ],
      "settings":{
        "foreground":"#b537f2",
        "fontStyle": "bold italic"
      }
    },
    {
      "scope":"constant.language.boolean.false",
      "settings":{
        "foreground":"#ff0000",
      }
    },
    {
      "scope":"constant.language.boolean.true",
      "settings":{
        "foreground":"#00ff00",
      }
    },
    {
      "name":"User-defined constant",
      "scope":[
        "constant.character",
        "constant.other"
      ],
      "settings":{
        "foreground":"#b537f2"
      }
    },
    {
      "name":"Variable",
      "scope":"variable",
      "settings":{
        "fontStyle":"bold italic",
        "foreground": "#18ffff"
      }
    },
    {
      "name":"Keyword",
      "scope":"keyword",
      "settings":{
        "fontStyle":"bold",
        "foreground":"#f92672"
      }
    },
    {
      "name":"Storage",
      "scope":"storage",
      "settings":{
        "fontStyle":"bold",
        "foreground":"#f92672"
      }
    },
    {
      "name":"Storage type",
      "scope":"storage.type",
      "settings":{
        "fontStyle":"bold",
        "foreground":"#b537f2"
      }
    },
    {
      "name":"Class name",
      "scope":"entity.name.class",
      "settings":{
        "fontStyle":"bold italic",
        "foreground":"#18ffff"
      }
    },
    {
      "name":"Inherited class",
      "scope":"entity.other.inherited-class",
      "settings":{
        "fontStyle":"bold italic",
        "foreground":"#3cb9fc"
      }
    },
    {
      "name":"Function name",
      "scope":[
        "entity.name.function",
        "entity.name.type",
        "support.function."
      ],
      "settings":{
        "fontStyle":"bold italic",
        "foreground":"#0084ff"
      }
    },
    {
      "name":"Function argument",
      "scope":"variable.parameter",
      "settings":{
        "fontStyle":"bold italic",
        "foreground":"#18ffff"
      }
    },
    {
      "name":"Tag name",
      "scope":"entity.name.tag",
      "settings":{
        "fontStyle":"bold",
        "foreground":"#b537f2"
      }
    },
    {
      "name":"Tag attribute",
      "scope":"entity.other.attribute-name",
      "settings":{
        "fontStyle":"italic bold",
        "foreground":"#3cb9fc"
      }
    },
    {
      "name":"Class and ID selectors",
      "scope":[
        "entity.other.attribute-name.class",
        "entity.other.attribute-name.id.css",
      ],
      "settings":{
        "fontStyle":"italic bold",
        "foreground":"#ffffff"
      }
    },
    {
      "name":"Library function",
      "scope":"support.function",
      "settings":{
        "fontStyle":"",
        "foreground":"#0084ff"
      }
    },
    {
      "name":"Library constant",
      "scope":[
        "support.constant",
        "constant.other"
      ],
      "settings":{
        "fontStyle":"bold italic",
        "foreground":"#b537f2"
      }
    },
    {
      "name":"Library class/type",
      "scope":[
        "support.type",
        "support.class",
        "support.variable"
      ],
      "settings":{
        "fontStyle":"bold italic",
        "foreground":"#18ffff"
      }
    },
    {
      "name":"Library variable",
      "scope":"support.other.variable",
      "settings":{
        "fontStyle":""
      }
    },
    {
      "name":"Invalid",
      "scope":"invalid",
      "settings":{
        "fontStyle":"bold",
        "foreground":"#f8f8f0"
      }
    },
    {
      "name":"Invalid deprecated",
      "scope":"invalid.deprecated",
      "settings":{
        "foreground":"#f8f8f0"
      }
    },
    {
      "name":"JSON String",
      "scope":[
        "meta.structure.dictionary.json",
        "string.quoted.double.json"
      ],
      "settings":{
        "foreground":"#ffffff"
      }
    },
    {
      "name":"diff.header",
      "scope":"meta.diff, meta.diff.header",
      "settings":{
        "foreground":"#75715e"
      }
    },
    {
      "name":"diff.deleted",
      "scope":"markup.deleted",
      "settings":{
        "foreground":"#f92672"
      }
    },
    {
      "name":"diff.inserted",
      "scope":"markup.inserted",
      "settings":{
        "foreground":"#a6e22e"
      }
    },
    {
      "name":"diff.changed",
      "scope":"markup.changed",
      "settings":{
        "foreground":"#e6db74"
      }
    },
    {
      "scope":"constant.numeric.line-number.find-in-files - match",
      "settings":{
        "foreground":"#0084ff"
      }
    },
    {
      "scope":"entity.name.filename.find-in-files",
      "settings":{
        "foreground":"#e6db74"
      }
    },
    {
      "name":"Language methods",
      "scope":[
        "variable.language"
      ],
      "settings":{
        "foreground":"#b537f2",
        "fontStyle": "bold"
      }
    },
    {
      "scope":"heading.1.markdown",
      "settings":{
        "foreground":"#a6e22e",
        "fontStyle":"bold"
      }
    },
    {
      "scope":"punctuation.definition.heading.markdown",
      "settings":{
        "foreground":"#a6e22e"
      }
    },
    {
      "scope":"entity.name.section.markdown",
      "settings":{
        "foreground":"#a6e22e"
      }
    },
    {
      "scope":"punctuation.definition.list.begin.markdown",
      "settings":{
        "foreground":"#b537f2"
      }
    },
    {
      "scope":"meta.image.inline.markdown",
      "settings":{
        "foreground":"#e6db74"
      }
    },
    {
      "scope":"markup.bold.markdown",
      "settings":{
        "foreground":"#ffffff",
        "fontStyle":"bold"
      }
    },
    {
      "scope":"markup.italic.markdown",
      "settings":{
        "foreground":"#ffffff",
        "fontStyle":"italic"
      }
    },
    {
      "scope":"markup.inline.raw.string.markdown",
      "settings":{
        "foreground":"#66d9ef"
      }
    },
    {
      "scope":"meta.separator.markdown",
      "settings":{
        "foreground":"#8f8f8f"
      }
    },
    {
      "scope":[
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end"
      ],
      "settings":{
        "foreground":"#b537f2",
        "fontStyle": "italic bold"
      }
    },
    {
      "scope":[
        "meta.brace.round",
        "punctuation.definition.parameters",
        "meta.brace.square",
        "punctuation.definition.block",
        "punctuation.definition.generic",
        "punctuation.section.property-list",
        "punctuation.definition.media-query",
        "punctuation.definition.dictionary",
        "punctuation.definition.array"
      ],
      "settings":{
        "foreground":"#ffffff",
        "fontStyle": "bold italic"
      }
    },
    {
      "scope":[
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#ffffff",
        "fontStyle": "bold"
      }
    },
    {
      "scope":[
        "variable.other.object",
        "variable.other.readwrite",
        "variable.parameter",
      ],
      "settings": {
        "foreground": "#18ffff",
        "fontStyle": "bold italic"
      }
    },
    {
      "scope":[
        "variable.other.object.property",
      ],
      "settings":{
        "foreground":"#3cb9fc",
        "fontStyle": "bold italic"
      }
    },
    {
      "scope":"variable.other.property",
      "settings":{
        "foreground":"#18ffff",
        "fontStyle": "bold italic"
      }
    },
    {
      "scope":[
        "meta.object-literal",
      ],
      "settings":{
        "foreground":"#b537f2",
        "fontStyle": "bold italic"
      }
    },
    {
      "scope":[
        "punctuation.separator.comma",
        "punctuation.accessor",
        "punctuation.terminator.statement",
        "punctuation.separator.key-value",
        "meta.property-list"
      ],
      "settings": {
        "foreground": "#ffffff",
        "fontStyle": "bold"
      }
    },
    {
      "scope":[
        "entity.name.tag.reference"
      ],
      "settings": {
        "foreground": "#f92672",
        "fontStyle": "bold"
      }
    },
    {
      "scope":[
        "source.ignore"
      ],
      "settings": {
        "foreground": "#ffffff",
        "fontStyle": "bold"
      }
    },
    {
      "scope":[
        "string.regexp punctuation.definition.string.begin",
        "string.regexp punctuation.definition.string.end"
      ],
      "settings": {
        "foreground": "#ffd700",
        "fontStyle": "bold"
      }
    }
  ],
