  "name": "Dark Matter",
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffffff80"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "punctuation.definition.string"
      ],
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#ffffff"
      }
    },
    {
      "scope": [
        "punctuation.definition.string.begin.markdown",
        "punctuation.definition.string.end.markdown"
      ],
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#ffff33"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#0084ff"
      }
    },
    {
      "name": "Built-in constant",
      "scope": [
        "constant.language",
        "entity.other.attribute-name.pseudo-class"
      ],
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#bf00ff"
      }
    },
    {
      "name": "JSON constant",
      "scope": [
        "constant.language.json"
      ],
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#3f00ff"
      }
    },
    {
      "name": "Booolean False",
      "scope": "constant.language.boolean.false",
      "settings": {
        "foreground": "#ff0000"
      }
    },
    {
      "name": "Booolean True",
      "scope": "constant.language.boolean.true",
      "settings": {
        "foreground": "#00ff00"
      }
    },
    {
      "name": "null/undefined",
      "scope": [
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#f700ff"
      }
    },
    {
      "name": "User-defined constant",
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#bf00ff"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#18ffff"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ff1476"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ff1476"
      }
    },
    {
      "name": "punctuation.assessor",
      "scope": "punctuation.accessor.optional",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ff1476"
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#3f00ff"
      }
    },
    {
      "name": "Class name",
      "scope": "entity.name.class",
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#18ffff"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#0084ff"
      }
    },
    {
      "name": "Function name",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.at-rule.keyframes",
        "meta.at-rule.keyframes.body",
        "meta.at-rule.media.header",
        "variable.parameter.keyframe-list"
      ],
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#6f00ff"
      }
    },
    {
      "name": "Types",
      "scope": [
        "entity.name.type",
        "support.type.primitive"
      ],
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#6f00ff"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#18ffff"
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#3f00ff"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "fontStyle": "italic bold",
        "foreground": "#6f00ff"
      }
    },
    {
      "name": "Class and ID selectors",
      "scope": [
        "entity.other.attribute-name.class",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "fontStyle": "italic bold",
        "foreground": "#bf00ff"
      }
    },
    {
      "name": "Library constant",
      "scope": [
        "support.constant",
        "constant.other"
      ],
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#3f00ff"
      }
    },
    {
      "name": "Library class/type",
      "scope": [
        "support.type",
        "support.class",
        "support.variable"
      ],
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#18ffff"
      }
    },
    {
      "scope": [
        "support.variable.property",
        "support.other.variable"
      ],
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#3f00ff"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#f8f8f0"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#f8f8f0"
      }
    },
    {
      "name": "JSON String",
      "scope": [
        "string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "JSON punctuation",
      "scope": [
        "meta.structure.dictionary.json"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ffffff"
      }
    },
    {
      "name": "diff.header",
      "scope": "meta.diff, meta.diff.header",
      "settings": {
        "foreground": "#75715e"
      }
    },
    {
      "name": "diff.deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#ff1476"
      }
    },
    {
      "name": "diff.inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#3f00ff"
      }
    },
    {
      "name": "diff.changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#e6db74"
      }
    },
    {
      "scope": "constant.numeric.line-number.find-in-files - match",
      "settings": {
        "foreground": "#0084ff"
      }
    },
    {
      "scope": "entity.name.filename.find-in-files",
      "settings": {
        "foreground": "#e6db74"
      }
    },
    {
      "name": "Language methods",
      "scope": [
        "variable.language"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#3f00ff"
      }
    },
    {
      "scope": "heading.1.markdown",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#3f00ff"
      }
    },
    {
      "scope": "punctuation.definition.heading.markdown",
      "settings": {
        "foreground": "#3f00ff"
      }
    },
    {
      "scope": "entity.name.section.markdown",
      "settings": {
        "foreground": "#3f00ff"
      }
    },
    {
      "scope": "punctuation.definition.list.begin.markdown",
      "settings": {
        "foreground": "#bf00ff"
      }
    },
    {
      "scope": "meta.image.inline.markdown",
      "settings": {
        "foreground": "#e6db74"
      }
    },
    {
      "scope": "markup.bold.markdown",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ffffff"
      }
    },
    {
      "scope": "markup.italic.markdown",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffffff"
      }
    },
    {
      "scope": "markup.inline.raw.string.markdown",
      "settings": {
        "foreground": "#66d9ef"
      }
    },
    {
      "scope": "meta.separator.markdown",
      "settings": {
        "foreground": "#8f8f8f"
      }
    },
    {
      "scope": [
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "fontStyle": "italic bold",
        "foreground": "#3f00ff"
      }
    },
    {
      "scope": [
        "meta.brace.round",
        "meta.brace.square",
        "punctuation.section",
        "punctuation.section.keyframes",
        "punctuation.definition",
        "punctuation.definition.string.template"
      ],
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#ffff33"
      }
    },
    {
      "scope": [
        "punctuation.definition.tag"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ffffff"
      }
    },
    {
      "scope": [
        "variable.other.object",
        "variable.other.readwrite",
        "variable.parameter"
      ],
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#18ffff"
      }
    },
    {
      "scope": [
        "variable.other.object.property"
      ],
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#0084ff"
      }
    },
    {
      "scope": "variable.other.property",
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#bf00ff"
      }
    },
    {
      "scope": [
        "meta.object-literal"
      ],
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#0057e9"
      }
    },
    {
      "scope": [
        "punctuation.separator.comma",
        "punctuation.separator.parameter",
        "punctuation.accessor",
        "punctuation.terminator",
        "punctuation.separator.key-value",
        "text.html",
        "meta.object.member"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ffffff"
      }
    },
    {
      "scope": [
        "support.constant.mathematical-symbols",
        "support.constant.mathematical-symbols",
        "meta.property-value"
      ],
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#6f00ff"
      }
    },
    {
      "scope": [
        "punctuation.separator.delimiter",
        "punctuation.separator.list.comma.css",
        "punctuation.separator.list.comma.scss"
      ],
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#ffffff"
      }
    },
    {
      "scope": [
        "entity.name.tag.reference",
        "meta.block"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ff1476"
      }
    },
    {
      "scope": [
        "source.ignore",
        "meta.scope.recipe.makefile",
        "source.env",
        "source.ini",
        "source.shell"
      ],
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Regex definition",
      "scope": [
        "string.regexp punctuation.definition.string.begin",
        "string.regexp punctuation.definition.string.end"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ff1476"
      }
    },
    {
      "name": "Meta variables",
      "scope": [
        "meta.var.expr"
      ],
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#3f00ff"
      }
    },
    {
      "scope": [
        "source.dockerfile"
      ],
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#3cb9fc"
      }
    },
    {
      "scope": [
        "punctuation.section.embedded.begin.php"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#8892be"
      }
    },
    {
      "scope": [
        "meta.jsx.children"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ffffff"
      }
    },
    {
      "scope": [
        "meta.property-list"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ffffff"
      }
    }
  ],
  "type": "dark"
}